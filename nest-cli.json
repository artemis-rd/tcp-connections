{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/tcp-connections-railway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/tcp-connections-railway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/tcp-connections-railway",
  "projects": {
    "tcp-connections-railway": {
      "type": "application",
      "root": "apps/tcp-connections-railway",
      "entryFile": "main",
      "sourceRoot": "apps/tcp-connections-railway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/tcp-connections-railway/tsconfig.app.json"
      }
    },
    "service-two": {
      "type": "application",
      "root": "apps/service-two",
      "entryFile": "main",
      "sourceRoot": "apps/service-two/src",
      "compilerOptions": {
        "tsConfigPath": "apps/service-two/tsconfig.app.json"
      }
    }
  }
}